    "version": "0.2.0",
    "configurations": [
        {
            "name": "Q2",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/q2/polytree-c/polytree",
            "args": ["l", "c", "a", "b"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/q2/polytree-c",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}